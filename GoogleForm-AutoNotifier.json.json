{
  "name": "GoogleForm-AutoNotifier",
  "nodes": [
    {
      "parameters": {
        "pollTimes": {
          "item": [
            {
              "mode": "everyMinute"
            }
          ]
        },
        "documentId": {
          "__rl": true,
          "value": "YOUR_GOOGLE_SPREADSHEET_URL",
          "mode": "url"
        },
        "sheetName": {
          "__rl": true,
          "value": "YOUR_SHEET_ID",
          "mode": "list",
          "cachedResultName": "Form responses 1",
          "cachedResultUrl": "YOUR_SHEET_URL_WITH_GID"
        },
        "event": "rowAdded",
        "options": {}
      },
      "type": "n8n-nodes-base.googleSheetsTrigger",
      "typeVersion": 1,
      "position": [
        -432,
        -16
      ],
      "id": "google-sheets-trigger-node",
      "name": "Google Sheets Trigger",
      "credentials": {
        "googleSheetsTriggerOAuth2Api": {
          "id": "YOUR_GOOGLE_SHEETS_CREDENTIAL_ID",
          "name": "Google Sheets Trigger account"
        }
      }
    },
    {
      "parameters": {
        "sendTo": "={{ $json.Email }}",
        "subject": "New Form Response Notification",
        "emailType": "text",
        "message": "=Hello {{ $json.Name }},\n\nThank you for your form submission!\n\nThis is an automated confirmation that we have received your response.\n\nWe will review your submission and get back to you if needed.\n\nBest regards,\n[YOUR_NAME/ORGANIZATION]",
        "options": {
          "appendAttribution": false
        }
      },
      "type": "n8n-nodes-base.gmail",
      "typeVersion": 2.1,
      "position": [
        -224,
        -16
      ],
      "id": "gmail-send-node",
      "name": "Send a message",
      "webhookId": "your-webhook-id",
      "credentials": {
        "gmailOAuth2": {
          "id": "YOUR_GMAIL_CREDENTIAL_ID",
          "name": "Gmail account"
        }
      }
    }
  ],
  "pinData": {},
  "connections": {
    "Google Sheets Trigger": {
      "main": [
        [
          {
            "node": "Send a message",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {
    "executionOrder": "v1"
  },
  "versionId": "template-version",
  "meta": {
    "templateCredsSetupCompleted": false,
    "instanceId": "template-instance"
  },
  "id": "template-workflow",
  "tags": []
}